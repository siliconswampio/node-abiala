{
  "name": "node-abiala",
  "version": "0.0.1",
  "description": "Node Bindings for abieos: Binary <> JSON conversion using ABIs.",
  "main": "abieos.js",
  "typings": "abieos.d.ts",
  "scripts": {
    "install": "node -e 'process.exit(0)'",
    "build": "cmake-js compile --cc /usr/bin/clang --cxx /usr/bin/clang++ && cp build/Release/node_abieos.node dist/abieos.node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eosrio/node-abieos.git"
  },
  "author": "EOS Rio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eosrio/node-abieos/issues"
  },
  "homepage": "https://github.com/eosrio/node-abieos#readme",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "cmake-js": "^6.1.0",
    "node-addon-api": "^3.2.1"
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "16.0.0",
    "arch": "x64"
  }
}
